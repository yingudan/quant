package com.ujuit.quant.firmoffer.dto;

import java.io.Serializable;

public class ChartHisDateDto implements Serializable {

	private static final long serialVersionUID = 1L;

	/**
	 * 累计收益
	 */
	private ChartPointDto accumulated;

	/**
	 * 对数收益
	 */
	private ChartPointDto logarithmic;

	/**
	 * 相对收益
	 */
	private ChartPointDto relative;

	/**
	 * 基准收益率
	 */
	private ChartPointDto benchmark;

	/**
	 * 当日买
	 */
	private ChartPointDto todayBuying;

	/**
	 * 当日售出
	 */
	private ChartPointDto todaySale;

	/**
	 * 当日盈亏
	 */
	private ChartPointDto todayPftAndLoss;

	public ChartPointDto getTodayPftAndLoss() {
		return todayPftAndLoss;
	}

	public void setTodayPftAndLoss(ChartPointDto todayPftAndLoss) {
		this.todayPftAndLoss = todayPftAndLoss;
	}

	public ChartPointDto getAccumulated() {
		return accumulated;
	}

	public void setAccumulated(ChartPointDto accumulated) {
		this.accumulated = accumulated;
	}

	public ChartPointDto getLogarithmic() {
		return logarithmic;
	}

	public void setLogarithmic(ChartPointDto logarithmic) {
		this.logarithmic = logarithmic;
	}

	public ChartPointDto getRelative() {
		return relative;
	}

	public void setRelative(ChartPointDto relative) {
		this.relative = relative;
	}

	public ChartPointDto getBenchmark() {
		return benchmark;
	}

	public void setBenchmark(ChartPointDto benchmark) {
		this.benchmark = benchmark;
	}

	public ChartPointDto getTodayBuying() {
		return todayBuying;
	}

	public void setTodayBuying(ChartPointDto todayBuying) {
		this.todayBuying = todayBuying;
	}

	public ChartPointDto getTodaySale() {
		return todaySale;
	}

	public void setTodaySale(ChartPointDto todaySale) {
		this.todaySale = todaySale;
	}

}
