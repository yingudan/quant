    margin-top:11px;
    margin-left:14px;
}
 .tools .statusA i{
	background:url(../../images/frontend/detail/statusA.png) no-repeat center center;
}
.tools .statusIng i{
	background:url(../../images/frontend/detail/statusIng.png) no-repeat center center;
}
.tools .statusNo i{
