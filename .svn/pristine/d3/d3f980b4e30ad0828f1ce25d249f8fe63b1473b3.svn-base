 .tools{height:35px;overflow:hidden;border-bottom:1px solid #e8eaeb;padding-bottom:12px;}
 .tools .return{display:block;width:40px;height:26px;margin:13px 10px 2px 25px;float:left;}
 .tools .titles{line-height:35px;font-size:16px;float:left;display:block;}
 .tools .stu{line-height:35px;display:block;margin-left:20px;float:left;position:relative;padding-left:18px;}
 .tools .stu i{display:block;width:15px;height:15px;position:absolute;left:0;top:7.5px;font-style:normal;line-height:15px;font-size:16px;}
.tools .btns{height:35px;float:left;padding:0 20px;}
.tools .btns a{line-height:35px;display:block;height:30px;text-align:center;border-radius:10px;float:left;}
.tools .btns a.ist{width:126px;height:35px;line-height:35px;font-size:14px;color:#fff;margin-right:20px;background:url(../../images/frontend/strategy/s_stop.png) no-repeat center center;}
.tools .btns a.ist2{width:124px;height:33px;font-size:14px;color:#008dff;margin-right:20px;border:1px solid #008dff;}
.tools .btns a.a{font-size:14px;color:#008DFF;}
.header .logo{
	margin-right:110px;
}
.main {
    width: 100%;
    margin: 0 auto;
    padding-top: 28px;
    overflow:hidden;
    padding-bottom:40px;
    background:#f2f5f6;
}
/*shangbaotai*/
.shangbaotai{text-align:center;overflow:hidden;}
.shangbaotai a{display:inline-block;width:160px;width:72px;height:35px;line-height:35px;text-align:center;color:#333;margin:0 20px;font-size:14px;font-weight:700;border-bottom:2px solid #fff;}
.shangbaotai a.on{color:#018EFF;border-bottom:2px solid #018EFF;}
.tools{
	padding-top:72px;
	width:100%;
}
.tools .status{
	display:block;
	height:30px;
	float:left;
	min-width:110px;
	overflow:hidden;
}
 .tools .status i{
	display: block;
    width: 75px;
