<!-- 头部 -->
<%@ page language="java" import="java.util.*" pageEncoding="utf-8"%>
<div class="detail-box">
    <h2 class="detail-title" id="shouyifengxian"></h2>
    <table class="table">
    <thead>
     <tr>
       <th width="20%">日期(年/月)</th>
       <th width="20%">1个月</th>
       <th width="20%">3个月</th>
       <th width="20%">6个月</th>
       <th width="20%">12个月</th>
     </tr><tr>
    </tr>
    </thead>
    <tbody id="shouyifengxian-content"></tbody>
    </table>
</div>
<script id="shouyifengxian-script" type="text/html">	
{{each data as item i}}
<tr>
 <td width="20%">{{item.time}}</td>
 <td width="20%">{{na(item.oneMonth)}}</td>
 <td width="20%">{{na(item.threeMonth)}}</td>
 <td width="20%">{{na(item.sixMonth)}}</td>
 <td width="20%">{{na(item.twelveMonth)}}</td>
</tr>
{{/each}}
</script>






